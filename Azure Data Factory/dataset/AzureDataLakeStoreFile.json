{
	"name": "AzureDataLakeStoreFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorageGen2",
			"type": "LinkedServiceReference"
		},
		"folder": {
			"name": "Framework"
		},
		"annotations": [],
		"type": "AzureBlobFSFile",
		"typeProperties": {
			"format": {
				"type": "JsonFormat",
				"filePattern": "setOfObjects"
			},
			"fileName": {
				"value": "@activity('uspGetCopyActivitySinkSQLToADLS').output.firstRow.CopyActivityDataADLSFileName",
				"type": "Expression"
			},
			"folderPath": {
				"value": "@activity('uspGetCopyActivitySinkSQLToADLS').output.firstRow.CopyActivityDataADLSFolderPath",
				"type": "Expression"
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}